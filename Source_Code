    "Method - 3 (White World Assumption)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 593,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Finding Brightest pixel over all vp and sq\n",
    "#color_patches[ill][vp][wb][patch][width][heigth][color_channel]\n",
    "brightest = np.zeros((3,3)) #[ill][[BGR]\n",
    "w = 0\n",
    "\n",
    "for i in range(lin_color_patches.shape[0]):\n",
    "    temp_max = 0\n",
    "    for v in range(lin_color_patches.shape[1]):\n",
    "        for p in range(lin_color_patches.shape[3]):\n",
    "            for width in range(lin_color_patches.shape[4]):\n",
    "                for height in range(lin_color_patches.shape[5]):\n",
    "                    B = lin_color_patches[i,v,w,p,width,height].sum()\n",
    "                    if B > temp_max:\n",
    "                        temp_max = B\n",
    "                        BGR = lin_color_patches[i,v,w,p,width,height]\n",
    "                        \n",
    "    brightest[i] = BGR\n",
    "\n",
    "for i in range(brightest.shape[0]):\n",
    "    brightest[i] = norm(brightest[i])"
   ]
  },
