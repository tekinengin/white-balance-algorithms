   "outputs": [],
   "source": [
    "#Parsing image names in order to create a easy-to-work image array\n",
    "def parse_imagename(name):\n",
    "    pos = 0\n",
    "    ill = int(name[pos])\n",
    "    pos += 2\n",
    "    if name[pos] == 'T':\n",
    "        vp = 0\n",
    "    elif name[pos] == '0':\n",
    "        vp = 1\n",
    "    elif name[pos] == '9':\n",
    "        vp = 2\n",
    "    elif name[pos] == '1':\n",
    "        vp = 3\n",
    "    elif name[pos] == '2':\n",
    "        vp = 4\n",
    "  \n",
    "    while name[pos] != '_':\n",
    "        pos += 1\n",
    "        \n",
    "    pos += 1\n",
    "    wb = int(name[pos])\n",
    "    \n",
    "    return ill,vp,wb"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 180,
   "metadata": {},
   "outputs": [],
   "source": [
    "def load_images_from_folder_color(folder):\n",
    "    images = []\n",
    "    for filename in os.listdir(folder):\n",
    "        img = cv2.imread(os.path.join(folder,filename))\n",
    "        if img is not None:\n",
    "            #obtaining 200x200 patch from manually cropped patches and averaging color channels\n",
    "            images.append((filename.replace('.jpeg',''), (img[:200,:200])))\n",
    "    return images"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 181,
   "metadata": {},
   "outputs": [],
   "source": [
    "def linearize_images(image,G):\n",
    "    for index in range(image.shape[-1]):\n",
    "        image[:,:,index] = image[:,:,index] ** G[index]\n",
    "    return image"
   ]
  },
