    "nBGR_white = np.array([0.3333,0.3333,0.3333])\n",
    "D_ill_1 = calc_Dill(nBGR_white,norm_BGR_over_vp[:,p])\n",
    "mtd1_nBGR = apply_wb(nBGR,D_ill_1)\n",
    "plot_nBGR(mtd1_nBGR,color,ill_symb,'Method-1')"
