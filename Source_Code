    "                plt.plot(nBGR[i,v,w,p,2],nBGR[i,v,w,p,1],color=color[p],marker=ill_symb[i],markersize=12)\n",
    "    plt.legend(legend)\n",
    "    plt.xlabel('R/(R+G+B)')\n",
    "    plt.ylabel('G/(R+G+B)')\n",
    "    plt.title('wb = {}'.format(w))\n",
    "    plt.xticks(np.arange(0, 1,0.1))\n",
    "    plt.yticks(np.arange(0, 0.5,0.05))\n",
    "    plt.grid()              \n",
    "plt.savefig('dist.png')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Part-4"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Color Constancy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 621,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Utility Functions\n",
    "def calc_Dill(ref_BGR,_BGR):\n",
    "    \n",
    "    def _calc_Dill(i1BGR, i2BGR):\n",
    "        temp = np.zeros(3)\n",
    "        temp[0] = i2BGR[0] / i1BGR[0]\n",
    "        temp[1] = i2BGR[1] / i1BGR[1]\n",
    "        temp[2] = i2BGR[2] / i1BGR[2]\n",
    "        return temp\n",
    "    D_ill = np.zeros((3,3)) # [ill][BGR]\n",
    "    for i in range(_BGR.shape[0]):\n",
    "        D_ill[i] = _calc_Dill(ref_BGR, _BGR[i])\n",
    "    \n",
    "    return D_ill\n",
    "\n",
    "#apply white balancing\n",
    "def apply_wb(nBGR,D_ill):\n",
    "    w = 0\n",
    "    wb_nBGR = np.zeros((3,5,2,4,3))\n",
    "    for i in range(nBGR.shape[0]): #ill\n",
    "        for v in range(nBGR.shape[1]): #vp\n",
    "            for p in range(nBGR.shape[3]): #patch\n",
    "                for c in range(nBGR.shape[4]): #color channel\n",
    "                    wb_nBGR[i,v,w,p,c] = nBGR[i,v,w,p,c] / D_ill[i,c]\n",
    "                wb_nBGR[i,v,w,p] = norm(wb_nBGR[i,v,w,p])\n",
    "    return wb_nBGR \n",
    "\n",
    "\n",
    "def plot_nBGR(wb_nBGR,color,ill_symb,title):\n",
    "    w = 0\n",
    "    plt.figure(figsize=[20,16])\n",
    "    for i in range(wb_nBGR.shape[0]): #ill\n",
    "        for v in range(wb_nBGR.shape[1]): #vp\n",
    "            for p in range(wb_nBGR.shape[3]): #patch\n",
    "                    plt.subplot(221)\n",
    "                    plt.plot(wb_nBGR[i,v,w,p,2],wb_nBGR[i,v,w,p,1],color=color[p],marker=ill_symb[i],markersize=12)\n",
    "    plt.legend(legend)\n",
    "    plt.xlabel('R/(R+G+B)')\n",
    "    plt.ylabel('G/(R+G+B)')\n",
    "    plt.xticks(np.arange(0, 1,0.1))\n",
    "    plt.yticks(np.arange(0, 0.5,0.05))\n",
    "    plt.title(title)\n",
    "    plt.grid()\n",
    "    for i in range(wb_nBGR.shape[0]): #ill\n",
    "        for v in range(wb_nBGR.shape[1]): #vp\n",
    "            for p in range(wb_nBGR.shape[3]): #patch\n",
    "                    plt.subplot(222)\n",
    "                    plt.plot(nBGR[i,v,w+1,p,2],nBGR[i,v,w+1,p,1],color=color[p],marker=ill_symb[i],markersize=12)\n",
    "    plt.legend(legend)\n",
    "    plt.xlabel('R/(R+G+B)')\n",
    "    plt.ylabel('G/(R+G+B)')\n",
    "    plt.xticks(np.arange(0, 1,0.1))\n",
    "    plt.yticks(np.arange(0, 0.5,0.05))\n",
    "    plt.title('wb = 1')             \n",
    "    plt.grid()\n",
    "    plt.savefig(title + '.png')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Method-1 (White Card)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 588,
   "metadata": {},
   "outputs": [],
   "source": [
    "avrg_BGR_over_vp = np.zeros((3,4,3)) #average BGR over vp with wb=0 [ill][patch][color_channel]\n",
    "w = 0\n",
    "for i in range(avrg_lin_patches.shape[0]): #ill\n",
    "    for p in range(avrg_lin_patches.shape[3]): #patch \n",
    "        for c in range(avrg_lin_patches.shape[4]): #color_channel BGR\n",
    "            avrg_BGR_over_vp[i,p,c] = avrg_lin_patches[i,:,w,p,c].sum() / avrg_lin_patches.shape[1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 589,
   "metadata": {},
   "outputs": [],
   "source": [
    "norm_BGR_over_vp = np.zeros((3,4,3))# [ill][patch][nBGR]\n",
    "for i in range(avrg_BGR_over_vp.shape[0]): #ill\n",
    "    for p in range(avrg_BGR_over_vp.shape[1]): #patch \n",
    "        norm_BGR_over_vp[i,p] = norm(avrg_BGR_over_vp[i,p])  "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 622,
