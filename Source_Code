   "source": [
    "avrg_BGR_over_vp_and_sq = np.zeros((3,3)) #average BGR over vp and sq with wb=0 [ill][color_channel]\n",
    "w = 0\n",
    "for i in range(avrg_BGR_over_vp.shape[0]): #ill\n",
    "    for c in range(avrg_BGR_over_vp.shape[2]): #color_channel BGR\n",
    "            avrg_BGR_over_vp_and_sq[i,c] = avrg_BGR_over_vp[i,:,c].sum() / avrg_BGR_over_vp.shape[1]\n",
    "            \n",
    "norm_BGR_over_vp_and_sq = np.zeros((3,3)) #norm BGR over vp and sq with wb=0 [ill][color_channel]\n",
    "w = 0\n",
    "for i in range(avrg_BGR_over_vp_and_sq.shape[0]): #ill\n",
    "    norm_BGR_over_vp_and_sq[i] = norm(avrg_BGR_over_vp_and_sq[i])\n"
   ]
