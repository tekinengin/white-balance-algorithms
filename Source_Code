  {
   "cell_type": "code",
   "execution_count": 187,
   "metadata": {},
   "outputs": [],
   "source": [
    "def avrg_color(image):\n",
    "    temp = np.zeros(3)\n",
    "    num_pix = 200 * 200\n",
    "    temp[0] = image[:,:,0].sum() // num_pix\n",
    "    temp[1] = image[:,:,1].sum() // num_pix\n",
    "    temp[2] = image[:,:,2].sum() // num_pix\n",
    "    return temp"
   ]
  },
