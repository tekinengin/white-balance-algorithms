   "source": [
    "Reading Captured Images and Cropping a Patch from Center\n",
    "\n",
    "Exposure Parameters Used:\n",
    "\n",
    "Time: [1/12000 - 1/30]\n",
    "Gain: Fixed ISO-64\n",
    "WB: Cloudy (wb=0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 599,
   "metadata": {},
   "outputs": [],
   "source": [
    "import cv2\n",
    "import os\n",
    "\n",
    "def load_images_from_folder(folder):\n",
    "    images = []\n",
    "    for filename in os.listdir(folder):\n",
    "        img = cv2.imread(os.path.join(folder,filename))\n",
    "        if img is not None:\n",
    "            images.append((int(filename.replace('.jpeg','')), img))\n",
    "    return images\n",
    "\n",
    "images = load_images_from_folder('WhitePaper/Raw')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "metadata": {},
   "outputs": [],
   "source": [
    "images = sorted(images, key = lambda x:x[0], reverse = True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 43,
   "metadata": {},
   "outputs": [],
   "source": [
    "mid_pix_hrztl = images[0][1].shape[0] // 2\n",
    "mid_pix_vrtl = images[0][1].shape[1] // 2\n",
    "crop_size = 500 // 2\n",
    "\n",
    "#We have 500 x 500 patch from images"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "metadata": {},
   "outputs": [],
   "source": [
    "crop_images = []\n",
    "for image in images:\n",
    "    exposure_time = image[0]\n",
    "    crop_img = image[1][mid_pix_hrztl - 2 * crop_size:mid_pix_hrztl, mid_pix_vrtl - 2 * crop_size:mid_pix_vrtl]\n",
    "    crop_images.append((exposure_time, crop_img))\n",
    "    #cv2.imwrite(\"WhitePaper/Cropped/{}.jpeg\".format(exposure_time), crop_img)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Calculating Average Values for Each Color Channel for different Exposure Times and Eliminating Saturated Images"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "metadata": {},
   "outputs": [],
   "source": [
    "imgs_T_BGR = []\n",
    "for image in crop_images:\n",
    "    B = image[1][:,:,0].sum() // (image[1].shape[0] * image[1].shape[1])\n",
    "    G = image[1][:,:,1].sum() // (image[1].shape[0] * image[1].shape[1])\n",
    "    R = image[1][:,:,2].sum() // (image[1].shape[0] * image[1].shape[1])\n",
    "    T = image[0]\n",
    "    TBGR = [T,B,G,R]\n",
    "    imgs_T_BGR.append(TBGR)"
   ]
  },
