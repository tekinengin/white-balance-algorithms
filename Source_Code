  {
   "cell_type": "code",
   "execution_count": 224,
   "metadata": {},
   "outputs": [],
   "source": [
    "def norm(BGR):\n",
    "    nBGR = np.zeros(3)\n",
    "    nBGR[0] = BGR[0] / BGR.sum()\n",
    "    nBGR[1] = BGR[1] / BGR.sum()\n",
    "    nBGR[2] = BGR[2] / BGR.sum()\n",
    "    return nBGR"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 227,
   "metadata": {},
   "outputs": [],
   "source": [
    "'''\n",
    "nRG[ill][vp][wb][patch][norm_color]\n",
    "\n",
    "ill: 0;1;2 --> Shadow Cast; Bulb; White Bulb \n",
    "vp: 0;1;2;3;4 --> Top; 0; 90; 180; 270\n",
    "wb: 0;1 --> On; Off\n",
    "patch: 0;1;2;3 --> light blue; lavender; yellow; white\n",
    "norm_color: 0;1;2 --> BGR \n",
    "'''\n",
    "nBGR = np.zeros((3,5,2,4,3))\n",
    "for i in range(avrg_lin_patches.shape[0]): #ill\n",
    "    for v in range(avrg_lin_patches.shape[1]): #vp\n",
    "        for w in range(avrg_lin_patches.shape[2]): #wb\n",
    "            for p in range(avrg_lin_patches.shape[3]): #patch\n",
    "                nBGR[i,v,w,p] = norm(avrg_lin_patches[i,v,w,p])          "
   ]
